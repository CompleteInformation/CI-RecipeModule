<Window x:Class="avaloniagui.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Types="clr-namespace:NicoVIII.CompleteInformation.Modules.Recipe.Types;assembly=recipes"
        Title="Complete Information - Recipes">
    <Grid ColumnDefinitions="1*, 1*">
        <ListBox>
            <ListBox.DataTemplates>
                <DataTemplate DataType="Types:Recipe">
                    <Grid Name="grid" Cursor="Hand">
                    </Grid>
                </DataTemplate>
            </ListBox.DataTemplates>
        </ListBox>
    </Grid>
</Window>
